<div style="display: none;"></div>
{{> hero}}
<section class="mainContent">
    <div class="container">
        {{#if contents}}
        {{#each contents}}
        <div class="row my-5">
            {{#each this}}
            {{#if this.title}}
            <div class="col-md-12">
                <h3>{{this.title}}</h3>
                {{{this.body}}}
            </div>
            <div class="col-md-4 col-lg-3 col-sm-6">
                <div class="galleryItem">
                    <picture class="galleryItem--picture">
                        <img class="galleryItem--picture__img"
                            src="{{this.src}}"
                            alt="{{this.alt}}"
                            loading="lazy" />
                    </picture>
                    <p class="galleryItem--description">{{this.alt}}</p>
                </div>
            </div>
            {{/if}}
            {{/each}}
        </div>
        {{/each}}
        {{/if}}
        <div class="row my-5">
            <div class="co-s-12 col-md-4 contactInfo">
                {{> contactInfo}}
            </div>
            <div class="col-sm-12 col-md-8 map">
                <iframe
                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1979.729168451159!2d24.855787415969264!3d60.25139814379178!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x468df64d58d8f7f1%3A0x89343ac576fac228!2sTuohiaukio%204%2C%2000410%20Helsingfors!5e0!3m2!1sfi!2sfi!4v1623325458211!5m2!1sfi!2sfi"
                    allow-fullscreen="true"
                    width="100%"
                    height="330px"
                    loading="lazy"></iframe>
            </div>
        </div>
    </div>
</section>
